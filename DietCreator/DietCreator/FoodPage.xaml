<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DietCreator.FoodPage">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="FoodImage" HorizontalOptions="Center" />
            <Label x:Name="CalloriesLabel"  FontSize="Large"/>
            <Label x:Name="ProteinLabel"  FontSize="Large"/>
            <Label x:Name="FatLabel"  FontSize="Large"/>
            <Label x:Name="CarbohydrateLabel"  FontSize="Large"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>